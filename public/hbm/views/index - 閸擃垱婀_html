	<script type="text/javascript" src="../js/echarts.min.js"></script>
	<script> 
		var myChart = echarts.init($(".main")[0]);
		var myChart1 = echarts.init($(".main")[1]);
		option = {
			xAxis:  {
				type: 'category',
				boundaryGap: false,
				data: ['00:00', '01:15', '02:30', '03:45', '05:00', '06:15', '07:30', '08:45', '10:00', '11:15', '12:30', '13:45', '15:00', '16:15', '17:30', '18:45', '20:00', '21:15', '22:30', '23:45']
				
			},
			yAxis: {
				type: 'value',
				axisLabel: {
					formatter: '{value}'
				},
				axisPointer: {
					snap: true
				},
				axisLine:{
					lineStayle:{
						width:1,
						color:'red'
					}
				},
			},
			visualMap: {
				show: false,
				dimension: 0,
				pieces: [{
					lte: 6,
					color: 'red'
				}, {
					gt: 6,
					lte: 8,
					color: 'red'
				}, {
					gt: 8,
					lte: 14,
					color: 'red'
				}, {
					gt: 14,
					lte: 17,
					color: 'red'
				}, {
					gt: 17,
					color: 'red'
				}]
			},
			grid:{
				top:5,
				x:40,
				x2:30,
				y2:36,
			},
			series: [{
				name:'用电量',
				type:'line',
				symbol: 'circle',     //设定为实心点
				symbolSize: 1,   //设定实心点的大小
				smooth: true,
				data: [15.33, 280, 250, 260, 270, 300, 550, 500, 400, 390, 380, 390, 400, 500, 600, 750, 800, 700, 600, 400],			
				itemStyle:{
					normal:{
						lineStyle:{
							width:'1'
						}
					}
				}
			}]
		};

		myChart.setOption(option);
		myChart1.setOption(option);

		$(function(){
			$(".coin_tbody").find(".coin_dealCount").addClass("hide");
			$(".coin_tbody").on("click","li",function(){
				$(".coin_tbody").find(".coin_dealCount").addClass("hide");
				$(this).find(".coin_dealCount").removeClass("hide");
			})
		})
   </script>
	

